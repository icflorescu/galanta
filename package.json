{
  "name": "galanta",
  "description": "A minimal UI library to make your Svelte apps look stylish",
  "version": "0.0.2",
  "scripts": {
    "generate-icons": "node ./scripts/generate-icons.js",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "publish": "npm version patch && npm run package && npm publish ./package && git push && git push --tags",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@tabler/icons": "^1.68.0",
    "@types/color": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "lodash-es": "^4.17.21",
    "prettier": "^2.6.2",
    "prettier-plugin-svelte": "^2.7.0",
    "sass": "^1.51.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "svelte2tsx": "^0.5.10",
    "svgo": "^2.8.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "color": "^4.2.3"
  },
  "type": "module",
  "author": {
    "name": "Ionut-Cristian Florescu",
    "email": "ionut.florescu@gmail.com",
    "url": "https://github.com/icflorescu"
  },
  "contributors": [
    {
      "name": "Irinel-Ramona Florescu",
      "email": "irinel.florescu@gmail.com",
      "url": "https://www.behance.net/iflorescu"
    },
    {
      "name": "Ionut-Cristian Florescu",
      "email": "ionut.florescu@gmail.com",
      "url": "https://github.com/icflorescu"
    }
  ],
  "keywords": [
    "ui",
    "library",
    "svelte",
    "sveltekit",
    "minimal",
    "styling",
    "pretty",
    "typescript",
    "javascript",
    "css"
  ]
}
